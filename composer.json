{
    "name": "bjoern-buettner/mini-mindmap",
    "description": "A small mindmap solution, that can run almost anywhere",
    "keywords": [
        "mindmap"
    ],
    "require": {
        "php": "^8",
        "ext-json": "*",
        "webmozart/assert": "^1.11.0",
        "nikic/fast-route": "^v1.3.0",
        "twig/twig": "^v3.10.3",
        "ramsey/uuid": "^4.7.6",
        "robmorgan/phinx": "^0.14.0 | ^0.15.0 | ^0.16.0",
        "ext-pdo": "*",
        "vlucas/phpdotenv": "^v5.6.0",
        "ext-dom": "*",
        "ext-gd": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9 | ^10 | ^11"
    },
    "autoload": {
        "psr-4": {
            "De\\Idrinth\\MiniMindmap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "De\\Idrinth\\MiniMindmap\\": "test"
        }
    },
    "config": {
        "platform": {
            "php": "8.0.0"
        }
    }
}
